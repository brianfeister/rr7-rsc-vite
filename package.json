{
    "name": "@hiogawa/vite-rsc-examples-react-router",
    "private": true,
    "license": "MIT",
    "type": "module",
    "scripts": {
        "dev": "cross-env NODE_ENV=development vite --port 5173",
        "build": "node scripts/check-typescript-only.js && vite build --app",
        "preview": "vite preview",
        "start": "cross-env NODE_ENV=production vite preview --port 5173",
        "typecheck": "tsc --noEmit",
        "lint": "eslint . --ext .ts,.tsx,.js,.jsx --report-unused-disable-directives --max-warnings 0",
        "lint:fix": "eslint . --fix",
        "lint:check-extensions": "node scripts/check-typescript-only.js",
        "lint:colors": "eslint src --ext .ts,.tsx,.js,.jsx --rule 'no-restricted-classnames: error'",
        "cf-dev": "vite -c ./cf/vite.config.ts",
        "cf-build": "cd cf && vite build",
        "cf-preview": "vite -c ./cf/vite.config.ts preview",
        "cf-release": "cd cf && wrangler deploy"
    },
    "dependencies": {
        "@hiogawa/vite-rsc": "latest",
        "@icons-pack/react-simple-icons": "^13.7.0",
        "@radix-ui/react-accordion": "^1.2.12",
        "@radix-ui/react-checkbox": "^1.3.3",
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-slot": "^1.2.3",
        "class-variance-authority": "^0.7.1",
        "client-only": "latest",
        "clsx": "^2.1.1",
        "commerce-sdk-isomorphic": "^3.4.0",
        "embla-carousel-react": "^8.6.0",
        "lucide-react": "^0.539.0",
        "react": "latest",
        "react-dom": "latest",
        "react-router": "0.0.0-experimental-23decd7bc",
        "server-only": "latest",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^4.1.4",
        "zustand": "^5.0.7",
        "sonner": "2.0.7"
    },
    "devDependencies": {
        "@cloudflare/vite-plugin": "^1.8.0",
        "@react-router/dev": "0.0.0-experimental-23decd7bc",
        "@tailwindcss/typography": "^0.5.16",
        "@tailwindcss/vite": "^4.1.4",
        "@types/node": "^24.0.3",
        "@types/react": "latest",
        "@types/react-dom": "latest",
        "@typescript-eslint/eslint-plugin": "^8.15.0",
        "@typescript-eslint/parser": "^8.15.0",
        "@vitejs/plugin-react": "latest",
        "@vitejs/plugin-rsc": "^0.4.20",
        "cross-env": "^10.0.0",
        "tailwindcss": "^4.1.4",
        "tw-animate-css": "^1.3.6",
        "typescript": "^5.8.3",
        "vite": "latest",
        "vite-plugin-devtools-json": "^0.4.1",
        "vite-plugin-inspect": "^11.2.0",
        "wrangler": "^4.22.0"
    },
    "overrides": {
        "react-router": "0.0.0-experimental-23decd7bc"
    }
